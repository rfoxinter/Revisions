\makeatletter
\def\comparestring#1#2{
    \edef\tempa{#1}
    \edef\tempb{#2}
    \ifx\tempa\tempb
        \expandafter\@firstoftwo
    \else
        \expandafter\@secondoftwo
    \fi
}
\def\compareint#1#2{
    \ifnum#1=#2\relax
        \expandafter\@firstoftwo
    \else
        \expandafter\@secondoftwo
    \fi
}
\makeatother